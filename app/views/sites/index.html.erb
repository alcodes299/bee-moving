<body id="noFun">
<div >
  <h1 class="sites">sites</h1>
  <% @sites.each do |s| %>
  <% if(s.claimed) %>
  <% echo = "claimed" %>
  <% else %>
  <% echo= "unclaimed"%>
  <% end %>
  <div class="columns small-6 large-4">
  <div id="siteHolder">
    <div>
      <div id="siteTitle"><%= s.title %></div>
      <span id="spanIt">
        <span id="showButton">
          <%= link_to "to show page", site_path(s), {:class => "button", :id => "genButton"}%>
        <div>
        <span id="claimation">
        <% if echo == "unclaimed" %>
        <%= "unclaimed" %>
        <% else %>
        <%= "claimed by beekeeper#: #{s.user_id}" %>
        <% end %>
        </div>

      </span>
    </div>
  </div>
  </div>
  <% end %>
</div>
</body>
