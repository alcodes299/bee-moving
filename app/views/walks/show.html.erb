<h1>Walk show page</h1>
<%= link_to "Back", walks_path %>
<div id="walkShowTitle">
  Walk-Name: <%= @walk.title %>
  <br>
  Description: <%= @walk.description %>
  <br>
  Location: <%= @walk.location %>
  <br>
  Creator Id#: <%= @walk.user_id %>
</div>
<p>
 <%= link_to "Delete", walk_path(@walk), method: :delete, data:{confirm: "You sure?"}  %>
 <%= link_to "Edit Walk", "/walks/#{@walk.id}/edit" %>
</p>
